dom\"\r\nimport { toast } from 'react-toastify';\r\nimport { Navbar } from 'react-bootstrap';\r\nconst CartPage = () => {\r\n  const [cartItems, setCartItems] = useState([]);\r\n  // const [refresh, setRefresh] = useState(false)\r\n\r\n  const navigate = useNavigate()\r\n\r\n  const removeProduct = (id)=>{\r\n    \r\n    axios.delete(\"http://127.0.0.1:8080/product/remCart/\" + id)\r\n        .then(res => {\r\n          getCartItem()\r\n        })\r\n        .catch(err => console.log(err))\r\n  }\r\n  const getCartItem = ()=>{\r\n    axios.get(\"http://127.0.0.1:8080/user/getCart/\" + sessionStorage.getItem(\"userId\"))\r\n        .then(res => {\r\n          console.log(res.data)\r\n          setCartItems(res.data)\r\n          navigate(\"/cart\")\r\n        })\r\n        .catch(err => console.log(err))\r\n  }\r\n  useEffect(() => {\r\n    getCartItem()\r\n  }, [])\r\n\r\n  const handleQuantityChange = (itemId, newQuantity) => {\r\n    let cart = {userId: sessionStorage.getItem(\"userId\"),\r\n                productId: itemId,\r\n                quantity: newQuantity}\r\n    axios.post(\"http://127.0.0.1:8080/product/cart/\", cart)\r\n        .then(res => {\r\n          toast.success(\"Product quantity changed in cart\");\r\n          getCartItem()\r\n        })\r\n    \r\n  };\r\n\r\n  const checkout = () => {\r\n    axios.get(\"http://127.0.0.1:8080/user/checkout/\" + sessionStorage.getItem(\"userId\"))\r\n        .then(res => navigate(\"/orders\"))\r\n        .catch(err => console.log(err))\r\n  }\r\n\r\n  const calculateTotal = () => {\r\n    return cartItems.reduce((total, item) => total + item.quantity * item.product.price, 0);\r\n  };\r\n\r\n  return (\r\n    <div className=\"container mt-5\">\r\n       <Navbar/>\r\n       <Link to='/shop' className='btn btn-primary mb-2' style={{marginRight:\"10px\"}}>Back To Shop</Link> \r\n      <h1>Shopping Cart</h1>\r\n\r\n      <table className=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th>Product</th>\r\n            <th>Price</th>\r\n            <th>Quantity</th>\r\n            <th>Total</th>\r\n            <th>Actions</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          {cartItems.map(item => (\r\n            <tr key={item.id}>\r\n              <td>{item.product.productName}</td>\r\n              <td>${item.product.price}</td>\r\n              <td>\r\n                <button\r\n                  className=\"btn btn-sm btn-secondary\"\r\n                  onClick={() =>\r\n                    handleQuantityChange(item.product.id, -1)\r\n                  }\r\n                  disabled={item.quantity === 1}\r\n                >\r\n                  -\r\n                </button>\r\n                {item.quantity}\r\n                <button\r\n                  className=\"btn btn-sm btn-secondary\"\r\n                  onClick={() =>\r\n                    handleQuantityChange(item.product.id, 1)\r\n                  }\r\n                >\r\n                  +\r\n                </button>\r\n              </td>\r\n              <td>${item.product.price * item.quantity}</td>\r\n              <td>\r\n                <button className=\"btn btn-sm btn-danger\" onClick={()=>{removeProduct(item.id)}} >Remove</button>\r\n              </td>\r\n            </tr>\r\n          ))}\r\n        </tbody>\r\n      </table>\r\n      <div className=\"text-right\">\r\n        <h5>Total Price: ${calculateTotal()}</h5>\r\n        <button className=\"btn btn-primary\" onClick={checkout}>Checkout</button>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CartPage;\r\n","/home/real/Desktop/PROJECT/Project/FrontEnd/React-InventoryMngnt/src/pages/Admin/NavigationBarAdmin.jsx",["176","177"],[],"/home/real/Desktop/PROJECT/Project/FrontEnd/React-InventoryMngnt/src/pages/Admin/Users/UsersApproved.jsx",[],[],"/home/real/Desktop/PROJECT/Project/FrontEnd/React-InventoryMngnt/src/pages/Admin/Users/UsersRemoved.jsx",[],[],"/home/real/Desktop/PROJECT/